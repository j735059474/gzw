<!--flag="0" 单独处理;flag="1" 查iufo表样;flag="2" 查事实表; 公式中"XX,2"表示XX公式计算后保留两位小数-->
<item key="vyear" dimension="" name="年">
  <source flag="1" code="QYBB" measure="month" row="" col="" id="$aa"/>
  <formula>
    '$aa'.substring(0,4)
  </formula>
</item>

<item key="vmonth" dimension="" name="月份">
  <source flag="1" code="QYBB" measure="month" row="" col="" id="$aa"/>
