    ($aa+$bb+$cc+$dd+$ee+$ff+$gg+$hh+$ii+$jj+$kk+$ll-nvl(($mm+$nn+$oo+$pp+$qq+$rr+$ss+$tt+$uu+$vv+$ww+$xx+$yy),0))/$zz*100
